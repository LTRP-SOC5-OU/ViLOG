{
  "name": "chibichilo-server",
  "version": "2.0.0",
  "main": "main.ts",
  "author": "Kohei Watanabe <kou029w@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "nodemon --ext js,ts,json --exec 'ncc run -t | pino-pretty'",
    "build": "run-s migrate build:prisma seed build:app",
    "build:app": "ncc build -t --target es2020",
    "build:prisma": "prisma generate",
    "migrate": "run-s migrate:save migrate:up",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "migrate:down": "prisma migrate down --experimental",
    "seed": "ncc run -t prisma/seed.ts",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@prisma/cli": "^2.12.0",
    "@prisma/client": "^2.12.0",
    "@quixo3/prisma-session-store": "^1.1.0",
    "@types/strict-uri-encode": "^2.0.0",
    "@vercel/ncc": "^0.25.1",
    "class-validator": "^0.12.2",
    "class-validator-jsonschema": "^2.0.3",
    "date-fns": "^2.16.1",
    "express-session": "^1.17.1",
    "fastify": "^3.7.0",
    "fastify-auth": "^1.0.1",
    "fastify-cookie": "^4.1.0",
    "fastify-cors": "^4.1.0",
    "fastify-formbody": "^5.0.0",
    "fastify-helmet": "^5.0.3",
    "fastify-multipart": "^3.3.0",
    "fastify-session": "^5.2.1",
    "fastify-swagger": "^3.5.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^4.3.0",
    "prisma-json-schema-generator": "^1.2.0",
    "strict-uri-encode": "^2.0.0",
    "ts-jest": "^26.4.4"
  }
}
