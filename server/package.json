{
  "name": "chibichilo-server",
  "version": "2.1.0",
  "main": "main.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/npocccties/chibichilo",
    "directory": "server"
  },
  "scripts": {
    "test": "jest",
    "dev": "run-p dev:*",
    "dev:app": "nodemon --ext js,ts,json --exec 'ncc run -t --no-asset-builds | pino-pretty'",
    "dev:db": "run-s build:prisma migrate seed",
    "build": "run-s build:prisma build:app",
    "build:app": "ncc build -t --no-asset-builds --target es2020",
    "build:prisma": "prisma generate",
    "migrate": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --skip-seed",
    "seed": "ncc run -t --no-asset-builds prisma/seed.ts",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@prisma/client": "^2.19.0",
    "@quixo3/prisma-session-store": "^3.0.1",
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^26.0.21",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node-schedule": "^1.3.2",
    "@types/serve-handler": "^6.1.0",
    "@types/spdx-expression-parse": "^3.0.0",
    "@types/strict-uri-encode": "^2.0.0",
    "@types/unzipper": "^0.10.3",
    "@vercel/ncc": "^0.27.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^2.2.0",
    "date-fns": "^2.19.0",
    "date-fns-tz": "^1.1.3",
    "express-session": "^1.17.1",
    "fastify": "^3.14.0",
    "fastify-auth": "^1.0.1",
    "fastify-cookie": "^5.2.0",
    "fastify-cors": "^5.2.0",
    "fastify-formbody": "^5.0.0",
    "fastify-helmet": "^5.3.0",
    "fastify-session": "^5.2.1",
    "fastify-swagger": "^4.4.2",
    "got": "^11.8.2",
    "jest": "^26.6.3",
    "json-schema-to-ts": "^1.6.4",
    "jsonwebtoken": "^8.5.1",
    "node-interval-tree": "^1.3.3",
    "node-schedule": "^2.0.0",
    "node-scp": "^0.0.14",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "openid-client": "^4.7.4",
    "outdent": "^0.8.0",
    "pino-pretty": "^4.7.1",
    "prisma": "^2.19.0",
    "prisma-json-schema-generator": "^1.2.1",
    "recursive-readdir-synchronous": "^0.0.4",
    "serve-handler": "^6.1.3",
    "spdx-expression-parse": "^3.0.1",
    "strict-uri-encode": "^2.0.0",
    "ts-jest": "^26.5.4",
    "unzipper": "^0.10.11"
  }
}
