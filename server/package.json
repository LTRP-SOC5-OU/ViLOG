{
  "name": "chibichilo-server",
  "version": "2.1.0",
  "main": "main.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/npocccties/chibichilo",
    "directory": "server"
  },
  "scripts": {
    "test": "jest",
    "dev": "run-p dev:*",
    "dev:app": "nodemon --ext js,ts,json --exec 'ncc run -t --no-asset-builds | pino-pretty'",
    "dev:db": "run-s build:prisma migrate seed",
    "build": "run-s build:prisma build:app",
    "build:app": "ncc build -t --no-asset-builds --target es2020",
    "build:prisma": "prisma generate",
    "migrate": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --skip-seed",
    "seed": "ncc run -t --no-asset-builds prisma/seed.ts",
    "zoom:import": "ncc run -t --no-asset-builds utils/zoom/importCli.ts",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@prisma/client": "^2.30.2",
    "@quixo3/prisma-session-store": "^3.1.0",
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node-schedule": "^1.3.2",
    "@types/serve-handler": "^6.1.1",
    "@types/spdx-expression-parse": "^3.0.1",
    "@types/ssh2": "^0.5.48",
    "@types/strict-uri-encode": "^2.0.0",
    "@types/unzipper": "^0.10.4",
    "@vercel/ncc": "^0.30.0",
    "ajv": "^8.8.2",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.1.0",
    "date-fns": "^2.23.0",
    "date-fns-tz": "^1.1.6",
    "express-session": "^1.17.2",
    "fastify": "^3.20.2",
    "fastify-auth": "^1.1.0",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.2",
    "fastify-formbody": "^5.1.0",
    "fastify-helmet": "^5.3.2",
    "fastify-session": "^5.2.1",
    "fastify-swagger": "^4.9.1",
    "got": "^11.8.2",
    "jest": "^27.1.0",
    "json-schema-to-ts": "^1.6.4",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^3.0.0",
    "node-interval-tree": "^1.3.3",
    "node-schedule": "^2.0.0",
    "node-ssh": "^12.0.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "openid-client": "^4.7.5",
    "outdent": "^0.8.0",
    "pino-pretty": "^6.0.0",
    "prisma": "^2.30.2",
    "recursive-readdir-synchronous": "^0.0.4",
    "search-query-parser": "^1.6.0",
    "serve-handler": "^6.1.3",
    "spdx-expression-parse": "^3.0.1",
    "strict-uri-encode": "^2.0.0",
    "ts-jest": "^27.0.5",
    "unzipper": "^0.10.11",
    "webpack": "^5.51.1",
    "yn": "^4.0.0"
  }
}
